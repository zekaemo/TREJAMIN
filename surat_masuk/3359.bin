:root {
    --default-font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto,
      Ubuntu, "Helvetica Neue", Helvetica, Arial, "PingFang SC",
      "Hiragino Sans GB", "Microsoft Yahei UI", "Microsoft Yahei",
      "Source Han Sans CN", sans-serif;
  }
  
  .main-container {
    overflow: hidden;
  }
  
  .main-container,
  .main-container * {
    box-sizing: border-box;
  }
  
  input,
  select,
  textarea,
  button {
    outline: 0;
  }
  
  /* Reset margin pada body */
  body {
    margin: 0;
    overflow-x: hidden;
  }
  
  /*STYLE MAIN PAGE*/
  .main-container {
    background: #f6f9ff;
  }
  
  /*STYLE HEADER*/
  .header-section {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
    padding: 0.5625rem 1.25rem; /* Padding (9px dan 20px konversi ke rem) */
    background: #ffffff;
    z-index: 200;
    box-shadow: 0 0.125rem 1.25rem rgba(1, 41, 112, 0.1); /* Box shadow (2px konversi ke rem) */
  }
  
  .content {
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
    flex-shrink: 0;
    gap: 0.3125rem; /* Jarak antar elemen (5px konversi ke rem) */
    position: relative;
    width: 17.5rem; /* Lebar (280px konversi ke rem) */
    padding: 0.5rem 0; /* Padding (8px konversi ke rem) */
    left: -0.9375rem; /* Posisi kiri (15px konversi ke rem) */
  }
  
  .link {
    flex-shrink: 0;
    position: relative;
    width: 17.5rem; /* Lebar (280px konversi ke rem) */
    height: 1.625rem; /* Tinggi (26px konversi ke rem) */
    z-index: 148;
  }
  
  /* STYLE TULISAN TREJAMIN */
  .tre-jamin {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    position: absolute;
    height: 2.1875rem; /* Tinggi (35px konversi ke rem) */
    top: -0.1875rem; /* Posisi atas ( -3px konversi ke rem) */
    left: 6.25rem; /* Posisi kiri (100px konversi ke rem) */
    color: #012970;
    font-family: Nunito, var(--default-font-family);
    font-size: 1.625rem; /* Ukuran font (26px konversi ke rem) */
    font-weight: 700;
    line-height: 1.625rem; /* Line height (26px konversi ke rem) */
    text-align: left;
    white-space: nowrap;
    z-index: 149;
  }
  
  .list {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    flex-wrap: nowrap;
    flex-grow: 1;
    flex-shrink: 0;
    flex-basis: 0;
    gap: 1.5625rem; /* Jarak antar elemen (25px konversi ke rem) */
    position: relative;
    min-width: 0;
    padding: 0 1rem 0 0; /* Padding (16px konversi ke rem) */
    z-index: 152;
  }
  
  .group {
    flex-shrink: 0;
    position: relative;
    width: 1.75rem; /* Lebar (28px konversi ke rem) */
    height: 1.6875rem; /* Tinggi (27px konversi ke rem) */
    z-index: 153;
  }
  
  .bell {
    position: absolute;
    width: 1.75rem; /* Lebar (28px konversi ke rem) */
    height: 1.6875rem; /* Tinggi (27px konversi ke rem) */
    top: 0;
    left: 0;
    background: url(./assets/images/ccf94286-2c8a-4b0c-a4f7-c4d306fd0972.png) no-repeat center;
    background-size: cover;
    z-index: 154;
  }
  
  .link-1 {
    flex-shrink: 0;
    position: relative;
    width: 8.585rem; /* Lebar (137.578px konversi ke rem) */
    height: 2.25rem; /* Tinggi (36px konversi ke rem) */
    z-index: 155;
  }
  
  .profile-image {
    position: absolute;
    width: 2.25rem; /* Lebar (36px konversi ke rem) */
    height: 2.25rem; /* Tinggi (36px konversi ke rem) */
    top: 50%;
    left: 0;
    background: url(./assets/images/62f859dc-e883-4e4b-a950-67f1ae449475.png) no-repeat center;
    background-size: cover;
    transform: translate(0, -50%);
    z-index: 2;
    border-radius: 1.125rem; /* Border radius (18px konversi ke rem) */
  }
  
  .text {
    position: absolute;
    width: 6.348rem; /* Lebar (101.578px konversi ke rem) */
    height: 1.3125rem; /* Tinggi (21px konversi ke rem) */
    top: 50%;
    left: 2.25rem; /* Posisi kiri (36px konversi ke rem) */
    transform: translate(0, -50%);
    z-index: 2;
  }
  
  .name {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    position: absolute;
    height: 1.3125rem; /* Tinggi (21px konversi ke rem) */
    top: 0;
    left: 0.5rem; /* Posisi kiri (8px konversi ke rem) */
    color: #012970;
    font-family: Open Sans, var(--default-font-family);
    font-size: 0.875rem; /* Ukuran font (14px konversi ke rem) */
    font-weight: 600;
    line-height: 1.3125rem; /* Line height (21px konversi ke rem) */
    text-align: left;
    white-space: nowrap;
    z-index: 2;
  }
  
  /* TEXT "SURAT MASUK" */
  .text-container h1 {
    color: #012970;
    font-family: Nunito, var(--default-font-family);
    font-size: 1.875rem; /* Ukuran font (30px konversi ke rem) */
    font-weight: 700;
    position: absolute;
    margin-top: 0;
    margin-left: 1.25rem; /* Posisi kiri (20px konversi ke rem) */
    width: 62.5rem; /* Lebar (1000px konversi ke rem) */
    z-index: 1;
  }
  
  
  /*TEXT 'SILAHKAN ISI SURAT MASUK' */
  .text-container h2 {
    color: #0c1018;
    font-family: Nunito, var(--default-font-family);
    font-size: 1.25rem; /* Ukuran font (20px konversi ke rem) */
    font-weight: 500;
    position: absolute;
    margin-top: 2.3125rem; /* Margin atas (37px konversi ke rem) */
    margin-left: 1.375rem; /* Margin kiri (22px konversi ke rem) */
    width: 62.5rem; /* Lebar (1000px konversi ke rem) */
    z-index: 1;
  }
  
  /* FONT PILIHAN BAGIAN */
  /* Mengatur font pada pilihan dalam select */
  select {
    font-family: 'Nunito', sans-serif; /* Menggunakan font Nunito */
    font-size: 0.875rem; /* Ukuran font (14px konversi ke rem) */
    color: #333; /* Warna teks */
    padding: 0.5rem; /* Padding (8px konversi ke rem) */
    border: 1px solid #ccc; /* Garis batas */
    border-radius: 0.25rem; /* Sudut border (4px konversi ke rem) */
    width: 100%; /* Lebar sesuai kebutuhan */
    box-sizing: border-box; /* Menghitung padding dan border dalam perhitungan lebar */
  }
  
  
  
