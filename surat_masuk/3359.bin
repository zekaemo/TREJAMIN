}

.tablee {
  border-radius: 0.5rem; /* 8px konversi ke rem */
  background-color: #ffffff;
  box-shadow: 0 0.25rem 0.5rem rgba(0, 0, 0, 0.1); /* 4px konversi ke rem untuk vertical shadow */
  padding: 1.25rem; /* 20px konversi ke rem */
  position: relative;
  height: auto;
  width: auto;
  box-sizing: border-box;
  max-width: 30rem; /* 480px konversi ke rem */
  min-width: 30rem; /* 480px konversi ke rem */
}

.tablee table {
  width: auto;
  border-collapse: collapse;
}

.tablee th, .tablee td {
  padding: 0.625rem 0.75rem; /* 10px 12px konversi ke rem */
  text-align: left;
  border: none;
  font-family: Nunito, var(--default-font-family);
}

.tablee th {
  background-color: #ffffff;
  color: #000000;
  font-weight: bold;
}

.tablee tr:nth-child(even) {
  background-color: #ffffff; /* Warna latar belakang baris genap */
}

.tablee td a {
  color: #000000;
  text-decoration: none;
}

        
    
        
          
          
          
    
.detail-button {
    display: inline-block;
    padding: 0.375rem 0.75rem; /* 6px 12px konversi ke rem */
    font-size: 0.875rem; /* 14px konversi ke rem */
    color: #ffffff;
    background-color: #012970;
    border-radius: 0.25rem; /* 4px konversi ke rem */
    text-decoration: none;
    text-align: center;
    border: 1px solid #012970;
    transition: background-color 0.2s, border-color 0.2s;
  }
  
  .detail-button:hover {
    background-color: #012970;
    border-color: #012970;
  }
  
  /* Styling untuk tabel data */
  table {
    width: 100%; /* Pastikan tabel mengisi lebar kontainer */
    border-collapse: collapse; /* Gabungkan border tabel */
    margin-top: 1.25rem; /* 20px konversi ke rem */
  }
  
  table th, table td {
    padding: 0.75rem 0.9375rem; /* 12px 15px konversi ke rem */
    border: 1px solid #f9f9f9; /* Border sel tabel */
    text-align: left; /* Rata kiri teks dalam sel tabel */
  }
  
  table th {
    background-color: #012970; /* Warna latar belakang header tabel */
    color: #ffffff; /* Warna teks header tabel (tambahan untuk visibilitas) */
    font-weight: bold; /* Berat font header tabel */
  }
  
  /* Styling untuk konten utama */
  .content-section {
    padding: 1.25rem; /* 20px konversi ke rem */
    background-color: #ffffff; /* Warna latar belakang konten */
    border-radius: 0.5rem; /* 8px konversi ke rem */
    box-shadow: 0 0.125rem 0.625rem rgba(0, 0, 0, 0.1); /* 2px konversi ke rem untuk vertical shadow */
    margin-left: 1.875rem; /* 30px konversi ke rem */
    position: absolute;
  }
  
  .btn-edit {
    display: inline-block;
    padding: 0.625rem 1.25rem; /* 10px 20px konversi ke rem */
    bottom: 1.25rem; /* 20px konversi ke rem */
    right: 9.0625rem; /* 145px konversi ke rem */
    background-color: #012970; /* Warna latar belakang tombol */
    color: #ffffff; /* Warna teks tombol */
    font-family: 'Open Sans', sans-serif; /* Font */
    font-weight: 600; /* Berat font */
    text-decoration: none; /* Hapus garis bawah */
    border-radius: 0.25rem; /* 4px konversi ke rem */
    border: 1px solid transparent; /* Border default */
    transition: background-color 0.3s, color 0.3s; /* Efek transisi */
    position: fixed; /* Menetapkan posisi absolut */
  }
  
  .btn-edit:hover {
    background-color: #0056b3; /* Warna latar belakang saat tombol ditekan */
  }

  .status-container {
    width: auto; /* Sesuaikan lebar sesuai kebutuhan */
    padding: 1.25rem; /* 20px konversi ke rem */
    border-radius: 0.5rem; /* 8px konversi ke rem */
    background-color: #ffffff;
    position: relative;
    box-shadow: 0 0.125rem 0.625rem rgba(0, 0, 0, 0.1); /* 2px konversi ke rem untuk vertical shadow */
    min-width: 30rem; /* 480px konversi ke rem */
    margin-top: 1.25rem; /* 20px konversi ke rem */
  }
  
  .status-title {
    text-align: center;
    color: #333;
    margin-bottom: 1.25rem; /* 20px konversi ke rem */
  }
  
  .timeline {
    border-left: 0.1875rem solid #012970; /* 3px konversi ke rem */
    padding-left: 1.25rem; /* 20px konversi ke rem */
  }
  
  .timeline-item {
    margin-bottom: 1.25rem; /* 20px konversi ke rem */
    position: relative;
  }
  
  .timeline-date {
    font-weight: bold;
    color: #012970;
    font-family: Nunito, var(--default-font-family);
  }
  
  .timeline-content {
    background-color: #fff;
    border: 0.0625rem solid #ccc; /* 1px konversi ke rem */
    border-radius: 0.3125rem; /* 5px konversi ke rem */
    padding: 0.625rem; /* 10px konversi ke rem */
    margin-top: 0.3125rem; /* 5px konversi ke rem */
    font-family: Nunito, var(--default-font-family);
  }
  
  
  
  /* CSS untuk elemen hr */
  .timeline-divider {
    border: none; /* Menghapus border default */
    border-top: 0.0625rem solid #012970; /* 1px konversi ke rem */
    margin: 0.625rem 0; /* 10px konversi ke rem */
  }
  
  .small-image {
    width: 2.5rem; /* 40px konversi ke rem */
    height: 2.5rem; /* 40px konversi ke rem */
    position: absolute;
    left: 0.625rem; /* 10px konversi ke rem */
    top: 0.125rem; /* 2px konversi ke rem */
  }
  
  .small-image2 {
    width: 2.5rem; /* 40px konversi ke rem */
    height: 2.5rem; /* 40px konversi ke rem */
    position: absolute;
    left: 3.1875rem; /* 51px konversi ke rem */
    top: 0; /* Tidak ada perubahan */
  }
  
  .lembar {
    border-radius: 0.5rem; /* 8px konversi ke rem */
    background-color: #ffffff; /* Warna latar belakang putih untuk kontainer */
    box-shadow: 0 0.25rem 0.5rem rgba(0, 0, 0, 0.1); /* Bayangan halus di sekitar kontainer */
    padding: 1.25rem; /* 20px konversi ke rem */
    padding-top: 0.3125rem; /* 5px konversi ke rem */
    position: relative;
    height: auto; /* Sesuaikan tinggi dengan konten */
    width: auto; /* Ikuti lebar konten */
    box-sizing: border-box; /* Sertakan padding dan batas dalam perhitungan lebar */
    max-width: 30rem; /* 480px konversi ke rem */
    min-width: 30rem; /* 480px konversi ke rem */
  }
  

  .lembar table {
    width: 28.125rem; /* 450px konversi ke rem */
    border-collapse: collapse; /* Menghapus batas antar sel */
  }
  
  .lembar th, .lembar td {
    padding: 0.625rem 0.75rem; /* 10px 12px konversi ke rem */
    text-align: left; /* Rata kiri untuk teks */
    border: none; /* Menghapus batas */
    font-family: Nunito, var(--default-font-family); /* Font yang digunakan */
  }
  
  .lembar th {
    background-color: #ffffff; /* Warna latar belakang untuk sel header */
    color: #000000; /* Warna teks hitam untuk header */
    font-weight: bold; /* Bold untuk teks header */
    width: 9.375rem; /* 150px konversi ke rem */
  }
  
  .lembar tr:nth-child(even) {
    background-color: #ffffff; /* Warna latar belakang terang untuk baris genap */
  }
  
  .lembar tr {
    max-width: 6.25rem; /* 100px konversi ke rem */
  }
  
  .lembar td a {
    color: #000000; /* Warna teks hitam untuk tautan */
    text-decoration: none; /* Menghapus garis bawah dari tautan */
    max-width: 18.75rem; /* 300px konversi ke rem */
  }
  
  .lembar h1 {
    font-size: 1.25rem; /* 20px konversi ke rem */
    margin-left: 0.625rem; /* 10px konversi ke rem */
  }
  
  .btn-kembali,
  .btn-edit {
    display: inline-block; /* Mengatur tampilan menjadi blok untuk memudahkan penataan */
    padding: 0.625rem 1.25rem; /* 10px 20px konversi ke rem */
    font-size: 1rem; /* 16px konversi ke rem */
    background-color: #012970; /* Warna latar belakang tombol */
    color: #ffffff; /* Warna teks tombol */
    font-family: 'Open Sans', sans-serif; /* Font */
    font-weight: 600; /* Berat font */
    text-decoration: none; /* Menghapus garis bawah */
    border-radius: 0.25rem; /* 4px konversi ke rem */
    border: 1px solid transparent; /* Border default */
    transition: background-color 0.3s, color 0.3s; /* Efek transisi */
    position: fixed; /* Menetapkan posisi tetap */
    z-index: 999; /* Agar tombol berada di atas elemen lainnya */
  }
  
  .btn-edit {
    bottom: 1.25rem; /* 20px konversi ke rem */
    right: 9.375rem; /* 150px konversi ke rem */
  }
  
  .btn-kembali {
    bottom: 1.25rem; /* 20px konversi ke rem */
    right: 1.875rem; /* 30px konversi ke rem */
  }
  
  .btn-kembali:hover,
  .btn-edit:hover {
    background-color: #0056b3; /* Warna latar belakang saat hover */
  }
  
